#  Check if the number has repeated digits.

num = int(input("Enter a number: "))
if num < 0:
    num = -num
digits_seen = set()
has_repeats = False
while num > 0:
    digit = num % 10
    if digit in digits_seen:
        has_repeats = True
        break
    digits_seen.add(digit)
    num //= 10
if has_repeats:
    print("The number has repeated digits.")
else:
    print("The number has no repeated digits.")
