# Check if all digits in a number are even

num = int(input("Enter a number: "))
if num < 0:
    num = -num
all_even = True 
while num > 0:
    digit = num % 10
    if digit % 2 != 0:  
        all_even = False
        break
    num //= 10
if all_even:
    print("All digits are even.")
else:
    print("Not all digits are even.")
